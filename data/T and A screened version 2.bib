issn = {03758427},
journal = {Veterinarni Medicina},
keywords = {,Latency period,Lytic activity,Mastitis,Phages,S. aureus},
number = {10},
pages = {421--426},
title = {{Characteristics of bacteriophages of the Staphylococcus aureus variant bovis}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85097464595&doi=10.17221%2F55%2F2020-VETMED&partnerID=40&md5=45dabd1d9f49b82d6b4c45cdb2c83341},
volume = {65},
