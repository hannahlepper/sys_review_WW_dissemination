journal = {Science of the Total Environment},
keywords = {,Antimicrobial resistance,Escherichia coli,Intestinal enterococci,River waters,Wastewaters},
number = {2},
pages = {365--372},
pmid = {19853889},
title = {{Antimicrobial resistance of fecal bacteria in waters of the Seine river watershed (France)}},
