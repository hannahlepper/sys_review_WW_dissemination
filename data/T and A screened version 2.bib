journal = {Zentralblatt fur Bakteriologie, Mikrobiologie und Hygiene. Serie B, Umwelthygiene, Krankenhaushygiene, Arbeitshygiene, praventive Medizin},
keywords = {Animals,Anti-Bacterial Agents,Drug Resistance, Microbial,Escherichia coli,Fresh Water,Humans,Metals,Sewage,Swine,Water Microbiology,drug effects,pharmacology},
