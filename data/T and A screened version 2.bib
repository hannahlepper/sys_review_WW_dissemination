pmid = {23221627},
title = {{Detection of quinolone-resistant Escherichia coli isolates belonging to clonal groups O25B:H4-B2-ST131 and O25B:H4-D-ST69 in raw sewage and river water in Barcelona, Spain}},
volume = {68},
