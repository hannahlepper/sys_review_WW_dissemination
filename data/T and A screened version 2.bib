keywords = {Anti-Bacterial Agents,Bacterial Typing Techniques,Cluster Analysis,DNA Fingerprinting,DNA, Bacterial,Drug Resistance, Bacterial,Electrophoresis, Gel, Pulsed-Field,Enterococcus faecium,Genotype,Gentamicins,Gram-Positive Bacterial Infections,Humans,Iran,Microbial Sensitivity Tests,Molecular Epidemiology,Ribotyping,Water Microbiology,classification,drug effects,genetics,isolation & purification,methods,microbiology,pharmacology},
language = {eng},
