journal = {FEMS microbiology ecology},
keywords = {,Anti-Bacterial Agents,Bacterial,Ciprofloxacin,Conjugation,DNA Gyrase,DNA Topoisomerase IV,Drug Resistance,Escherichia coli,Escherichia coli Infections,Escherichia coli Proteins,Genetic,Interspersed Repetitive Sequences,Multilocus Sequence Typing,Multiple,Plasmids,Rivers,Waste Water,beta-Lactam Resistance,beta-Lactamases,drug effects,drug therapy,genetics,isolation & purification,microbiology,pharmacology},
