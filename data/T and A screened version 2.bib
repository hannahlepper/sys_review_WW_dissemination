pages = {1368--1376},
pmid = {30507460},
title = {{Outbreak investigation of ceftriaxone-resistant Salmonella enterica serotype Typhi and its risk factors among the general population in Hyderabad, Pakistan: a matched case-control study}},
volume = {18},
