doi = {10.1089/mdr.2019.0039},
journal = {Microbial Drug Resistance},
number = {3},
pages = {227--237},
title = {{High Phenotypic and Genotypic Diversity of Enterococcus faecium from Clinical and Commensal Isolates in Third Level Hospital}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85081646332&doi=10.1089%2Fmdr.2019.0039&partnerID=40&md5=b0093d4f3442ab81a44067920ccfb6ba},
