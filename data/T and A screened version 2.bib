doi = {10.1016/j.scitotenv.2017.11.287},
issn = {18791026},
journal = {Science of the Total Environment},
keywords = {,Antibiotic uptake,Antibiotics resistance,Hospital wastewater,Human exposure,Irrigation water,Wastewater stabilization pond},
pages = {293--305},
pmid = {29216470},
title = {{Occurrence and risk assessment of antibiotics in water and lettuce in Ghana}},
