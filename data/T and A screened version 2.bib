keywords = {,Anti-Bacterial Agents,Bacterial,Bacterial Proteins,Brazil,Drug Resistance,Enterobacteriaceae,Gene Expression Regulation,Humans,Lakes,Multiple,Sanitation,Urban Health,Water Microbiology,drug effects,genetics,metabolism,pharmacology},
language = {eng},
