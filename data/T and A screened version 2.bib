journal = {Microbial Drug Resistance},
keywords = {,Anti-Bacterial Agents,Bacterial Typing Techniques,California,Cities,DNA,Escherichia coli,Escherichia coli Proteins,Gene Expression,Genetic Variation,Genotype,Humans,Klebsiella pneumoniae,Phylogeny,Plasmids,Sequence Analysis,Waste Water,Water Microbiology,Wetlands,beta-Lactam Resistance,beta-Lactamases,beta-Lactams,chemistry,classification,drug effects,genetics,isolation & purification,metabolism,microbiology,pharmacology},
