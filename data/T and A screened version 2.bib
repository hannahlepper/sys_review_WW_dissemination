doi = {10.1038/s41396-017-0030-8},
issn = {17517370},
journal = {ISME Journal},
number = {3},
pages = {681--691},
pmid = {29374269},
title = {{The widespread dissemination of integrons throughout bacterial communities in a riverine system}},
