journal = {Communications Biology},
keywords = {,Anti-Bacterial Agents,Bacteria,Bacterial,Drug Resistance,Feces,Humans,Metagenomics,Models,Prevalence,Public Health Surveillance,Sewage,Statistical,drug effects,genetics,methods,microbiology,pharmacology},
language = {eng},
